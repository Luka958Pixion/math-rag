{
  "uid": "pbs-pro-job",
  "title": "PBS Pro Job",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "name": "job_id",
        "type": "query",
        "label": "Job ID",
        "datasource": {
          "type": "prometheus",
          "uid": "hpc"
        },
        "query": "label_values(resources_used_cpu_percent, job_id)",
        "includeAll": true,
        "allValue": ".*",
        "multi": false
      }
    ]
  },
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "hpc"
      },
      "type": "timeseries",
      "title": "CPU Usage (%)",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "resources_used_cpu_percent{job_id=~\"$job_id\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "hpc"
      },
      "type": "timeseries",
      "title": "Total CPU Time",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "resources_used_cpu_seconds_total{job_id=~\"$job_id\"}",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "hpc"
      },
      "type": "timeseries",
      "title": "Number of CPUs",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "resources_used_num_cpus{job_id=~\"$job_id\"}",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "hpc"
      },
      "type": "timeseries",
      "title": "Memory Usage",
      "gridPos": {
        "x": 12,
        "y": 6,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "resources_used_mem_bytes{job_id=~\"$job_id\"} / 1024 / 1024 / 1024",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "GiB"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Memory Usage (GiB): \\$job_id"
            },
            "properties": [
              {
                "id": "axis",
                "value": "right"
              }
            ]
          }
        ]
      }
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "hpc"
      },
      "type": "timeseries",
      "title": "Virtual Memory",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "resources_used_vmem_bytes{job_id=~\"$job_id\"} / 1024 / 1024 / 1024",
          "refId": "E"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "GiB"
        }
      }
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "hpc"
      },
      "type": "timeseries",
      "title": "Wall Clock Time",
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "resources_used_wall_seconds_total{job_id=~\"$job_id\"}",
          "refId": "F"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    }
  ]
}
